** Number of spectras in individual programs
*** stars only
    SELECT  p.programname, count(s.specObjID)
    INTO mydb.stars
    FROM SpecPhotoAll s, platex p
    WHERE s.specObjID is not null
    AND s.plateid = p.plateid
    AND s.class = 'STAR'
    group by p.programname
